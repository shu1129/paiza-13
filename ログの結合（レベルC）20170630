#include <iostream>
#include <stdio.h>
#include <string>
#include <string.h>
#include <sstream>
#include <vector>
#include <math.h>

int main(void){
  int N;
  std::string s2;
  std::cin >> N;
  std::cin >> s2;

  int P = 0;

  for ( int i = 0; i <= N; i++) {
    if ( s2[i] == "L") {
      int a = pow(10,i);
      P = P + a*(i+1);
    }else{
      P = P*10 + i;
  }
  }
  std::cout << P << std::endl;
}
